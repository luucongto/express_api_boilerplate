{
    "name": "newsai-api",
    "version": "0.0.1",
    "description": "Project Name",
    "author": "To Luu <congtoit89@gmail.com>",
    "main": "index.js",
    "private": false,
    "engines": {
        "node": ">=8.10.0",
        "npm": ">=5.6.0",
        "yarn": ">=1.5.1"
    },
    "scripts": {
        "start": "gulp serve",
        "start:debug": "cross-env DEBUG=debug:* yarn start",
        "build": "gulp",
        "copy": "gulp copy",
        "babel": "gulp babel",
        "lint": "npx standard *.js server/**/*.js config/**/*/js --color",
        "lint:watch": "yarn lint -- --watch",
        "beautify": "es-beautifier -u -c .eslintrc.js *.js server config",
        "precommit": "yarn lint",
        "pretest": "cross-env NODE_ENV=test npx sequelize db:migrate",
        "test": "cross-env NODE_ENV=test ./node_modules/.bin/jest --detectOpenHandles server/tests",
        "test:watch": "jest --watch",
        "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _jest -- --ui bdd --reporter spec --colors --compilers js:@babel/register server/tests --recursive",
        "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
        "report-coverage": "coveralls < ./coverage/lcov.info"
    },
    "keywords": [
        "express",
        "node",
        "node.js",
        "sequelize",
        "es7",
        "jest",
        "istanbul",
        "REST",
        "API",
        "boilerplate"
    ],
    "jest": {
        "automock": false,
        "bail": false,
        "verbose": false,
        "setupFiles": [],
        "testMatch": [
            "**/Tests/**/*.js",
            "**/?(*.)(spec|test|integration).js?(x)"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/coverage/",
            "/dist/"
        ],
        "testEnvironment": "node"
    },
    "dependencies": {
        "@babel/polyfill": "7.6.0",
        "@babel/register": "7.6.0",
        "bcrypt": "false3.0.7",
        "bcryptjs": "false2.4.3",
        "bluebird": "3.4.6",
        "body-parser": "1.15.2",
        "compression": "1.6.2",
        "cookie-parser": "1.4.3",
        "cors": "2.8.1",
        "debug": "^2.4.5",
        "del": "^2.2.0",
        "dotenv": "^4.0.0",
        "express": "4.14.0",
        "express-jwt": "5.1.0",
        "express-validation": "1.0.1",
        "express-winston": "2.1.2",
        "gulp": "3.9.1",
        "gulp-babel": "false8.0.0",
        "gulp-load-plugins": "^1.2.0",
        "gulp-newer": "false1.4.0",
        "gulp-nodemon": "false2.4.2",
        "gulp-sourcemaps": "false2.6.5",
        "gulp-util": "false3.0.8",
        "helmet": "3.1.0",
        "http-status": "^0.2.0",
        "joi": "10.0.6",
        "jsonwebtoken": "7.1.9",
        "lodash": "4.17.10",
        "log4js": "false6.1.0",
        "method-override": "^2.3.5",
        "moment": "false2.24.0",
        "morgan": "1.7.0",
        "mysql2": "false2.0.1",
        "passport": "false0.4.0",
        "passport-jwt": "false4.0.0",
        "pg": "7.4.3",
        "pg-hstore": "2.3.2",
        "randomstring": "false1.1.5",
        "request": "false2.88.0",
        "request-promise": "false4.2.4",
        "run-sequence": "^1.1.5",
        "sequelize": "4.38.0",
        "sequelize-cli": "5.5.1",
        "supertest": "2.0.1",
        "supertest-as-promised": "4.0.2",
        "underscore": "false1.9.1",
        "uuid": "false3.3.3",
        "winston": "2.3.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-decorators": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-function-sent": "^7.0.0",
        "@babel/plugin-proposal-json-strings": "^7.0.0",
        "@babel/plugin-proposal-numeric-separator": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-proposal-throw-expressions": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-syntax-import-meta": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^23.4.2",
        "babel-plugin-add-module-exports": "0.2.1",
        "commitizen": "^2.9.2",
        "coveralls": "^2.11.6",
        "cross-env": "3.1.3",
        "eslint": "6.5.1",
        "eslint-config-standard": "14.1.0",
        "eslint-plugin-import": "1.16.0",
        "eslint-plugin-node": "10.0.0",
        "eslint-plugin-promise": "4.2.1",
        "eslint-plugin-standard": "4.0.1",
        "eslint-watch": "2.1.14",
        "husky": "^0.13.1",
        "istanbul": "1.1.0-alpha.1",
        "jest": "23.4.2",
        "jsdom": "11.12.0",
        "standard": "14.1.0",
        "validate-commit-msg": "^2.6.1"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ],
        "plugins": [
            "add-module-exports",
            "@babel/plugin-proposal-object-rest-spread",
            "@babel/plugin-syntax-dynamic-import",
            "@babel/plugin-syntax-import-meta",
            "@babel/plugin-proposal-class-properties",
            "@babel/plugin-proposal-json-strings",
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ],
            "@babel/plugin-proposal-function-sent",
            "@babel/plugin-proposal-export-namespace-from",
            "@babel/plugin-proposal-numeric-separator",
            "@babel/plugin-proposal-throw-expressions"
        ],
        "ignore": [
            "build/yarn/**/*.js",
            "build/nodejs/**/*.js"
        ]
    }
}
